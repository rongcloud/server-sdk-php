{
  "add": {
    "docs": "http://rongcloud.cn/docs/server.html#sensitiveword_add",
    "url": "sensitiveword/add",
    "params": {
      "rule": {
        "type": 1,
        "keyword": "法轮功",
        "replace": "***"
      }
    },
    "response": {
      "success": {
        "code": "200",
        "msg": "成功返回"
      },
      "fail": {
        "1005": {
          "code": "1005",
          "msg": "{{name}} 数量超限，{{name}} >= {{min}} 且 {{name}} <= {{max}}"
        },
        "1002": {
          "code": "1002",
          "msg": "{{name}} 参数为必传项"
        },
        "1501": {
          "code": "1501",
          "msg": "参数类型不正确，请检查参数类型，应该为 {{type}} 当前为 {{currentType}} "
        },
        "1004": {
          "code": "1004",
          "msg": "签名错误, 请排查 Appkey、Secret 是否正确"
        },
        "1008": {
          "code": "1008",
          "msg": "调用频率超限，每秒钟限制 100 次，调整频率上限请移步至专有云 http://www.rongcloud.cn/pricing#pay"
        }
      }
    }
  },
